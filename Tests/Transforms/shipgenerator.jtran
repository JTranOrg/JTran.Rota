  "#variable(ShipNames)":
  {
    "#iterate($NumShips * 3, ships)": 
    {
      "name":             "#(trim(pickrandom($names) + ' ' + pickrandom($nameSuffixes)))"
    }
  },

  "#variable(currentDate)":   "2512-5-7",

  "#variable(officers)": 
  [
    { "Name": "Captain",                "Weight": 1 },
    { "Name": "First Officer",          "Weight": 2 },
    { "Name": "Engineering Officer",    "Weight": 3 },
    { "Name": "Operations Officer",     "Weight": 3 },
    { "Name": "Communications Officer", "Weight": 4 },
    { "Name": "Medical Officer",        "Weight": 4 }
  ],

  "#variable(crewPositions)": 
  [
    { "Name": "Propulsion Technician" },
    { "Name": "Maintenance Technician" },
    { "Name": "Medical Technician" },
    { "Name": "Communications Technician" },
    { "Name": "Pilot" }
  ],

  "#variable(crewTitles)": 
  [
    { "Name": "Chief /s/",          "MaxYears": 15, "Weight": 5 },
    { "Name": "/s/ First Class",    "MaxYears": 8,  "Weight": 6 },
    { "Name": "/s/ Second Class",   "MaxYears": 6,  "Weight": 7 },
    { "Name": "/s/ Second Class",   "MaxYears": 6,  "Weight": 7 },
    { "Name": "/s/ Third Class",    "MaxYears": 2,  "Weight": 8 },
    { "Name": "/s/ Third Class",    "MaxYears": 2,  "Weight": 8 },
    { "Name": "/s/ Third Class",    "MaxYears": 2,  "Weight": 8 }
  ],

  "#iterate($NumShips, [])": 
  {
    "#variable(registration)":  "#(pickrandom($origins))",

    "Name":             "#(pickrandomunique($ShipNames.ships, 'names').name)",
    "ClassName":        "#(pickrandom(['Super Heavy Freighter', 'Heavy Freighter', 'Medium Freighter', 'Light Freighter', 'Large Passenger', 'Medium Passenger', 'Small Passenger', 'Large Cruiser', 'Medium Cruiser', 'Small Cruiser']))",
    "Registration":     "#($registration)",
    "YearBuilt":        "#(randominteger(2384, 2512))",
    "Length":           "#(randominteger(85, 245))",
    "Width":            "#(randominteger(24, 64))",
    "Height":           "#(randominteger(12, 32))",
    "NumEngines":       "#(randominteger(1, 4) * 2)",
    "MaxWarp":          "#(randominteger(4, 10))",
    "PodAttachment":    "#(pickrandom(['external', 'internal'])",
    "MaxPods":          "#(randominteger(8, 32) * 2)",

    "#array(Crew)": 
    {
      "#foreach($officers, {})":
      {
        "#variable(age_in_days)":   "#(randominteger(2 * 365, 15 * 365) * -1)",

        "CrewId":                   "#(pickrandomunique($persons, 'persons').Id)",
        "Position":                 "#(Name)",
        "PositionWeight":           "#(Weight)",
        "Started":                  "#(formatdatetime(adddays($currentDate, $age_in_days), 'yyyy-MM-dd'))"
      },

      "#foreach($crewPositions)":
      {
        "#variable(CrewPosition)":    "#(Name)",

        "#foreach($crewTitles, {})":
        {
          "#variable(age_in_days)":   "#(randominteger(1 * 365, MaxYears * 365) * -1)",

          "CrewId":                   "#(pickrandomunique($persons, 'persons').Id)",
          "Position":                 "#(replaced(Name, '/s/', $CrewPosition))",
          "PositionWeight":           "#(Weight)",
          "Started":                  "#(formatdatetime(adddays($currentDate, $age_in_days), 'yyyy-MM-dd'))"
        }
      }
    }
  }
}
